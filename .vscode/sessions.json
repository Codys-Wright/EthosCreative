{
  "$schema": "https://cdn.statically.io/gh/nguyenngoclongdev/cdn/main/schema/v10/terminal-keeper.json",
  "theme": "tribe",
  "active": "default",
  "activateOnStartup": true,
  "keepExistingTerminals": false,
  "sessions": {
    "default": [
      {
        "name": "Dev",
        "autoExecuteCommands": true,
        "icon": "rocket",
        "color": "terminal.ansiGreen",
        "commands": [
          "pnpm dev"
        ]
      },
      {
        "name": "Install",
        "icon": "package",
        "color": "terminal.ansiYellow",
        "commands": [
          "echo 'Run: pnpm install --auto-accept-peers to install dependencies'"
        ]
      },
      {
        "name": "Database",
        "icon": "database",
        "color": "terminal.ansiBlue",
        "commands": [
          "echo 'Ready for drizzle commands'"
        ]
      },
      {
        "name": "Docker",
        "icon": "container",
        "color": "terminal.ansiCyan",
        "commands": [
          "echo 'Ready for docker commands'"
        ]
      },
      {
        "name": "Git",
        "icon": "git-branch",
        "color": "terminal.ansiMagenta",
        "commands": [
          "echo 'Ready for git commands'"
        ]
      }
    ],
    "saved-session": [
      {
        "name": "connect",
        "commands": [
          ""
        ]
      }
    ]
  }
}